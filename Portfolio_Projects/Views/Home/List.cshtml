@model IEnumerable<Portfolio_Projects.LinkConverter>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LongLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShortLinkExtension)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpirationDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LongLink)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShortLinkExtension)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExpirationDate)
        </td>
    </tr>
}

</table>
